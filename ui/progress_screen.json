  "label_with_percentage": {
    "type": "panel",
    "anchor_from": "center", 
    "anchor_to": "center",
    "size": [ "100%", 30 ],
    "controls": [
      {
        "stack": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%c", "100%" ],
          "controls": [
            {
              "persen_panel@progress.persen_panel": {}
            },
            {
              "label_panel": {
                "type": "panel",
                "size": [ "100%c", 30 ],
                "controls": [
                  {
                    "percent_text": {
                      "type": "label",
                      "layer": 40,
                      "alpha": "@zxyn.alpha_0",
                      "anchor_from": "bottom_middle", 
                      "anchor_to": "bottom_middle",
                      "localize": false,
                      "font_size": "large",
                      "text": "%"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "modal_progress_screen@progress_screen": {
    "$screen_content": "progress.modal_screen_content"
  },

  "neko_progress_screen_text": {
    "type": "label",
    "size": [ "100%", "default" ],
    "text_alignment": "center",
    "max_size": [ "100%", "30px" ],
    "font_scale_factor": "$ukuran_font",
    "font_type": "$gaya_font"
  },
  
  "neko_progress_title_text": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "progress_title": {
          "size": [ "100%", "100%" ],
          "type": "stack_panel",
          "controls": [
            {
              "panel_loading": {
                "type": "panel",
                "anchor_from": "center",
                "anchor_to": "center",
                "size": [ "100%", "fill" ],
                "controls": [
                  {
                    "loading_spin@progress.loading_spin": {
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "size": [ "100%" , 15 ]
                    }
                  }
                ] 
              }
            },
            {
              "panel_label": {
                "type": "panel",
                "size": [ "100%", "45px" ],
                "controls": [
                  {
                    "stack_panel_label": {
                      "type": "stack_panel",
                      "size": [ "100%", "100%" ],
                      "controls": [
                        {
                          "panel_label": {
                            "type": "panel",
                            "size": [ "100%", "100%c" ],
                            "controls": [
                              {
                                "progress_title_text@progress.neko_progress_screen_text": {
                                  "$ukuran_font": 1,
                                  "$gaya_font": "EudoxusSans-Bold",
                                  "text": "#title_text",
                                  "bindings": [
                                    {
                                      "binding_name": "#title_text"
                                    }
                                  ]
                                }
                              }
                            ]
                          }
                        },
                        { "spacer_2": { "type": "panel", "size": [ 3, 3 ]}},
                        {
                          "panel_label_2": {
                            "type": "panel",
                            "size": [ "100%", "100%c" ],
                            "controls": [
                              {
                                "progress_bar_text@progress.neko_progress_screen_text": {
                                  "$ukuran_font": 0.75,
                                  "$gaya_font": "EudoxusSans-Medium",
                                  "text": "#progress_text",
                                  "bindings": [
                                    { "binding_name": "#progress_text" }
                                  ]
                                }
                              }
                            ]
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },


