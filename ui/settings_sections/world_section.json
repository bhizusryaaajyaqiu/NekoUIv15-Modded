  "experimental_button@settings_common.section_toggle_base": {
    "$toggle_view_binding_name": "experimental_toggle",
    "$glyph_texture": "textures/neko/glyph_icon/experimental",
    "$glyph_color_texture": "textures/neko/glyph_icon/experimental_color",
    "$glyph_size": [ 18, 18 ],
    "$button_text": "createWorldScreen.experimtalSettings",
    "$toggle_property_bag": {
      "#panel_title": "createWorldScreen.experimtalSettings"
    }
  },

  "experimental_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "experimental_toggle",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ],
    "controls": [
      {
        "experimental_toggles_label@settings_common.option_group_label": {
          "$text": "createWorldScreen.experimentalgameplay",
          // Ignore education toggle in education since education doesn't allow experiments presently
          "ignored": "$education_edition"
        }
      },
      {
        "experimental_toggles_label_info@settings_common.option_group_label": {
          "$text": "createWorldScreen.experimentalgameplayinfo",
          // Ignore education toggle in education since education doesn't allow experiments presently
          "ignored": "$education_edition"
        }
      },
      {
        "experimental_toggles": {
          "type": "stack_panel",
          "orientation": "vertical",
          "size": [ "100%", "100%c" ],
          // Ignore education toggle in education since education doesn't allow experiments presently
          "ignored": "$education_edition",
          "factory": {
            "name": "experimental_toggle_factory",
            "control_name": "world_section.experimental_toggle"
          },

          "collection_name": "experimental_toggles",
          "bindings": [
            {
              "binding_name": "#experimental_toggle_count",
              "binding_name_override": "#collection_length"
            }
          ]
        }
