                    "gamemode_type@anjim.gamemode_type": {}
                  },
                  { "spacer_03" :  {"type": "panel", "size": [ 5 , 5 ] }}
                ]
              }
            }
          ]
        }
      },
      { "spacer_09" :  {"type": "panel", "size": [ 3 , 3] }},
      {
        "personal_gamemode_type@custom_settings_section.neko_radio_panel":{
