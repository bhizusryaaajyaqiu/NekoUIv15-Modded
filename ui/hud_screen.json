  "root_panel/chat_stack": {
    "modifications": [
      {
        "array_name": "controls",
        "operation": "remove",
        "where": { "player_position@hud.player_position": {}}
      }
    ]
  },

  "boss_health_panel_v2": {
    "type": "stack_panel",
    "orientation": "vertical",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "boss_hud_padding": {
          "type": "panel",
          "size": [ "100%", 20 ],
          "visible": "#boss_hud_padding", //Make this visible to move the boss hud panel down
          "bindings": [
            {
              "binding_name": "#boss_hud_padding",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "boss_hud_touch_padding": {
          "type": "panel",
          "size": [ "100%", "15%" ],
          "visible": "#boss_hud_touch_padding", //Make this visible to move the boss hud panel down
          "bindings": [
            {
              "binding_name": "#boss_hud_touch_padding",
              "binding_name_override": "#visible",
              "binding_condition": "always"
            }
          ]
        }
      },
      {
        "boss_hud_panel": {
          "type": "panel",
          "controls": [
            {
              "boss_health_grid@boss_health_grid": {
                "offset": [ 0, 2 ]
              }
            }
          ]
        }
      }

    ]
  },

  "anim_item_name_a": {
    "anim_type": "offset",
    "easing": "in_expo",
    "duration": 0.5,
    "from": [ 0, -30 ],
    "to": [ 0, -35 ]
  },


  "anim_item_name_text_alpha_out": {
    "anim_type": "alpha",
    "easing": "in_expo",
    "destroy_at_end": "$destroy_id",
    "duration": 1,
    "from": 1.0,
    "to": 0
  },

  "item_name_text@hud.item_name_text_root": {
    "offset": "@hud.anim_item_name_a"
  },

  "jukebox_popup_text@hud.item_name_text_root": {
    "$text_color": "$jukebox_text_color",
    "$text_binding": "#jukebox_text",
    "offset": [ 0, -34 ] // default y = -34
  },

  "root_panel": {
    "modifications": [       
      {
        "array_name": "controls",
        "operation": "insert_back",
        "value": { "durability_ping_panel@zxyn_hud.durability_ping_panel": {} }
      },     
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [{"kordinat_player@zxyn_hud.kordinat_player": {}}] 
      },
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [{ "boss_health_panel_v2@hud.boss_health_panel_v2": {}}] 
      },
      {
        "array_name": "controls",
        "operation": "remove",
        "where":
        {
          "boss_health_panel": {
            "type": "stack_panel",
            "orientation": "vertical",
            "size": [ "100%", "100%" ],
            "controls": [
              {
                "boss_hud_padding": {
                  "type": "panel",
                  "size": [ "100%", 20 ],
                  "visible": "#boss_hud_padding", //Make this visible to move the boss hud panel down
                  "bindings": [
                    {
                      "binding_name": "#boss_hud_padding",
                      "binding_name_override": "#visible"
                    }
                  ]
                }
              },
              {
                "boss_hud_touch_padding": {
                  "type": "panel",
                  "size": [ "100%", "15%" ],
                  "visible": "#boss_hud_touch_padding", //Make this visible to move the boss hud panel down
                  "bindings": [
                    {
                      "binding_name": "#boss_hud_touch_padding",
                      "binding_name_override": "#visible",
                      "binding_condition": "always"
                    }
                  ]
                }
              },
              {
                "boss_hud_panel": {
                  "type": "panel",
                  "controls": [
                    {
                      "boss_health_grid@boss_health_grid": {
                        "offset": [ 0, 2 ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        } 
      },
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [{"item_aux_bg@zxyn_hud.item_aux_bg": {}}] 
      },
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [{ "test_panel_2@zxyn_hud.test_panel_2": {}}] 
      },
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [{ "test_panel3@zxyn_hud_content.test_panel3": {}}] 
      },
      {
        "array_name": "controls",
        "operation": "remove",
        "where": [{"emote_expediate_helpers@hud.emote_expediate_helpers": {}}] 
